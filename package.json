{
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "build": "tsc",
    "start": "node dist/app.js",
    "typeorm": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:migrate": "yarn typeorm -- migration:run -d src/db/data-source",
    "db:migrate:create": "yarn typeorm -- migration:create",
    "db:migrate:generate": "yarn typeorm -- migration:generate -d src/db/data-source",
    "db:migrate:revert": "yarn typeorm -- migration:revert -d src/db/data-source"
  },
  "dependencies": {
    "@adminjs/fastify": "4.2.0",
    "@adminjs/typeorm": "5.0.1",
    "@fastify/session": "11.1.0",
    "adminjs": "7.8.17",
    "date-fns": "4.1.0",
    "fastify": "5.5.0",
    "pg": "8.16.3",
    "pino": "9.9.0",
    "react": "18.3.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.25"
  },
  "devDependencies": {
    "@types/node": "24.2.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "pino-pretty": "13.1.1",
    "tslib": "2.8.1",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  }
}
